plex_rcs:
  servers:
    - host: localhost
      port: 32400
      token: TOKEN
  backend: vfs
  media_root: /mnt/unionfs
  log_file: "/var/lib/rclone/rclone.log"
  env:
    LD_LIBRARY_PATH: "/usr/lib/plexmediaserver/lib"
    PLEX_MEDIA_SERVER_APPLICATION_SUPPORT_DIR: "/var/lib/plexmediaserver/Library/Application Support"
